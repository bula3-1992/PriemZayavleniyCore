package utils;

/**
 *
 * @author 003-0823
 */
public class FilePair {

    private String xmlName;
    private byte[] xmlContent;
    private String p7sName;
    private byte[] p7sContent;

    public FilePair() {
    }

    public void setXML(String name, byte[] content) {
        this.xmlName = name;
        this.xmlContent = content;
    }

    public void setP7S(String name, byte[] content) {
        this.p7sName = name;
        this.p7sContent = content;
    }

    public String getXmlName() {
        return xmlName;
    }

    public byte[] getXmlContent() {
        return xmlContent;
    }

    public String getP7sName() {
        return p7sName;
    }

    public byte[] getP7sContent() {
        return p7sContent;
    }

}
